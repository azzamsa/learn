    "@tsconfig/node18": "^18.2.2",
    "@types/node": "^18.18.5",
    "@urql/vue": "^1.1.2",
    "@vitejs/plugin-vue": "^4.5.0",
    "@vue/eslint-config-typescript": "^12.0.0",
    "@vue/tsconfig": "^0.4.0",
    "autoprefixer": "^10.4.16",
    "eslint": "^8.54.0",
    "eslint-plugin-vue": "^9.18.1",
    "npm-run-all2": "^6.1.1",
    "postcss": "^8.4.31",
    "tailwindcss": "^3.3.5",
    "typescript": "~5.3.2",
    "vite": "^5.0.3",
    "vue-tsc": "^1.8.22"
