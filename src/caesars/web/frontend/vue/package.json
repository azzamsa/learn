{
  "name": "frontend",
  "type": "module",
  "version": "0.0.0",
  "private": true,
  "packageManager": "pnpm@8.11.0",
  "scripts": {
    "dev": "vite --port 3333 --open",
    "lint": "eslint .",
    "fmt": "eslint . --fix",
