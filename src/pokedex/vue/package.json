{
  "name": "pokedex-vue",
  "type": "module",
  "version": "1.3.0",
  "private": true,
  "packageManager": "pnpm@8.11.0",
  "scripts": {
    "dev": "vite --port 3333 --open",
    "preview": "vite preview",
    "fmt:tailwind": "rustywind --write .",
    "fmt:style": "eslint . --fix",
    "fmt": "run-s fmt:tailwind fmt:style",
    "lint:style": "eslint .",
    "lint:type": "vue-tsc --noEmit -p tsconfig.vitest.json --composite false",
    "lint": "run-s lint:style lint:type",
    "test:unit": "vitest --run",
    "test:e2e": "start-server-and-test preview http://localhost:3333/ 'cypress open --e2e'",
    "test:e2e:ci": "start-server-and-test preview http://localhost:3333/ 'cypress run --e2e'",
    "test": "run-p test:unit test:e2e:ci",
    "build:vite": "vite build",
    "build": "run-p lint build:vite"
  },
  "dependencies": {
    "@intlify/unplugin-vue-i18n": "^1.5.0",
    "@vueuse/core": "^10.6.1",
    "@vueuse/head": "^2.0.0",
    "daisyui": "^4.4.14",
    "lodash-es": "^4.17.21",
    "pinia": "^2.1.7",
    "vue": "^3.3.9",
    "vue-i18n": "9.2.2",
    "vue-router": "^4.2.5"
  },
  "devDependencies": {
    "@antfu/eslint-config": "^2.1.1",
    "@iconify/json": "^2.2.149",
    "@rushstack/eslint-patch": "^1.6.0",
    "@types/jsdom": "^21.1.6",
    "@types/lodash-es": "^4.17.12",
    "@types/node": "^20.10.0",
    "@vitejs/plugin-vue": "^4.5.0",
    "@vue/eslint-config-prettier": "^8.0.0",
    "@vue/eslint-config-typescript": "^12.0.0",
    "@vue/test-utils": "^2.4.3",
    "@vue/tsconfig": "^0.4.0",
    "autoprefixer": "^10.4.16",
    "cypress": "^13.6.0",
    "eslint": "^8.54.0",
    "eslint-plugin-cypress": "^2.15.1",
    "eslint-plugin-vue": "^9.18.1",
    "jsdom": "^23.0.0",
    "npm-run-all": "^4.1.5",
    "postcss": "^8.4.31",
    "start-server-and-test": "^2.0.3",
    "tailwindcss": "^3.3.5",
    "typescript": "~5.3.2",
    "unplugin-auto-import": "^0.17.1",
    "unplugin-icons": "^0.17.4",
    "unplugin-vue-components": "^0.25.2",
    "vite": "^5.0.3",
    "vite-plugin-pages": "^0.32.0",
    "vite-plugin-vue-layouts": "^0.8.0",
    "vitest": "^0.34.6",
    "vue-tsc": "^1.8.22"
  }
}
