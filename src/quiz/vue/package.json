{
    "name": "quiz-vue",
    "version": "1.1.0",
    "type": "module",
    "private": true,
    "packageManager": "pnpm@8.11.0",
    "scripts": {
        "dev": "vite --port 3333 --open",
        "preview": "vite preview",
        "fmt:tailwind": "rustywind --write .",
        "fmt:style": "eslint . --fix",
        "fmt": "run-s fmt:tailwind fmt:style",
        "lint:style": "eslint .",
        "lint:type": "vue-tsc --noEmit -p tsconfig.vitest.json --composite false",
        "lint": "run-s lint:style lint:type",
        "test:unit": "vitest --run",
        "test:e2e": "start-server-and-test preview http://localhost:4173/ 'cypress open --e2e'",
        "test:e2e:ci": "start-server-and-test preview http://localhost:4173/ 'cypress run --e2e'",
        "test": "run-p test:unit test:e2e:ci",
        "build:vite": "vite build",
        "build": "run-p lint build:vite"
    },
    "dependencies": {
        "@vueuse/core": "^10.5.0",
        "@vueuse/head": "^2.0.0",
        "daisyui": "^3.9.2",
        "lodash-es": "^4.17.21",
        "pinia": "^2.1.7",
        "vue": "^3.3.4",
        "vue-i18n": "9.5.0",
        "vue-router": "^4.2.5"
    },
    "devDependencies": {
        "@antfu/eslint-config": "^2.1.1",
        "@iconify/json": "^2.2.128",
        "@intlify/unplugin-vue-i18n": "^1.4.0",
        "@rushstack/eslint-patch": "^1.5.1",
        "@tsconfig/node18": "^18.2.2",
        "@types/jsdom": "^21.1.3",
        "@types/lodash-es": "^4.17.9",
        "@types/node": "^20.8.6",
        "@vitejs/plugin-vue": "^4.4.0",
        "@vue/eslint-config-prettier": "^8.0.0",
        "@vue/eslint-config-typescript": "^12.0.0",
        "@vue/test-utils": "^2.4.1",
        "@vue/tsconfig": "^0.4.0",
        "autoprefixer": "^10.4.16",
        "cypress": "^13.3.1",
        "eslint": "^8.54.0",
        "eslint-plugin-cypress": "^2.15.1",
        "eslint-plugin-vue": "^9.17.0",
        "jsdom": "^22.1.0",
        "npm-run-all": "^4.1.5",
        "postcss": "^8.4.31",
        "rustywind": "^0.20.0",
        "start-server-and-test": "^2.0.1",
        "tailwindcss": "^3.3.3",
        "typescript": "~5.2.2",
        "unplugin-auto-import": "^0.16.6",
        "unplugin-icons": "^0.17.0",
        "unplugin-vue-components": "^0.25.2",
        "vite": "^4.4.11",
        "vite-plugin-pages": "^0.31.0",
        "vite-plugin-vue-layouts": "^0.8.0",
        "vitest": "^0.34.6",
        "vue-tsc": "^1.8.19"
    },
    "pnpm": {
        "peerDependencyRules": {
            "allowedVersions": {
                "vite": "^4.0.0"
            }
        }
    }
}
