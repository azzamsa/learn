{
  "name": "qwiz",
  "version": "0.0.1",
  "private": true,
  "packageManager": "pnpm@8.11.0",
  "scripts": {
    "dev": "vite dev --port 3333 --open",
    "build": "paraglide-js compile --project ./project.inlang && vite build",
    "preview": "vite preview",
    "format": "pnpm run format:style && pnpm run lint:fix",
    "lint": "pnpm run lint:style && pnpm run lint:type",
    "test": "pnpm run test:integration && npm run test:unit",
    "format:style": "prettier --write . --log-level silent",
    "lint:fix": "eslint . --fix",
    "lint:style": "prettier --check . && eslint .",
    "lint:type": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
    "lint:type:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
    "test:integration": "playwright test",
    "test:unit": "vitest --run"
  },
  "devDependencies": {
    "@iconify-json/heroicons": "^1.1.15",
    "@inlang/paraglide-js": "1.0.0-prerelease.15",
    "@inlang/paraglide-js-adapter-vite": "^1.0.1",
    "@playwright/test": "^1.40.1",
    "@skeletonlabs/skeleton": "2.5.1",
    "@skeletonlabs/tw-plugin": "0.2.4",
    "@sveltejs/adapter-auto": "^2.1.1",
    "@sveltejs/kit": "^1.27.6",
    "@tailwindcss/typography": "0.5.10",
    "@types/node": "18.19.0",
    "@typescript-eslint/eslint-plugin": "^6.13.1",
    "@typescript-eslint/parser": "^6.13.1",
    "autoprefixer": "10.4.16",
    "eslint": "^8.55.0",
    "eslint-config-prettier": "^9.0.0",
    "eslint-plugin-svelte": "^2.35.1",
    "heroicons:bars-3": "link:@iconify-json/heroicons:bars-3",
    "heroicons:bars3": "link:@iconify-json/heroicons:bars3",
    "postcss": "8.4.31",
    "prettier": "^3.1.0",
    "prettier-plugin-svelte": "^3.1.2",
    "prettier-plugin-tailwindcss": "^0.5.7",
    "svelte": "^4.2.8",
    "svelte-check": "^3.6.2",
    "tailwindcss": "3.3.5",
    "tslib": "^2.6.2",
    "typescript": "^5.3.2",
    "unplugin-icons": "^0.18.0",
    "vite": "^5.0.4",
    "vite-plugin-tailwind-purgecss": "0.1.4",
    "vitest": "^0.34.6"
  },
  "type": "module"
}
